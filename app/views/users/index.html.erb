<h2 align = "center">Interns</h2>


 <ul class="collection">
    <% @users.each do |user| %>
        <li class="collection-item avatar">
          <%if user.profileImage.present?%>
            <%= image_tag user.profileImage_url.to_s, class: 'material-icons circle medium profImage line-height' %>
          <%else%>
            <i class="material-icons circle">account_circle</i>
          <%end%>
          <span class="title"><%=link_to user.fullname, user_path(user.id)%></span>
          <!--<p>First Line <br>
             Second Line
          </p>
          <a href="#!" class="secondary-content"><i class="material-icons">grade</i></a> -->
        </li>
    <%end%>
  </ul>
 
<div align = "center">
    <%= will_paginate @users %>
</div>
